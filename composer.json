{
    "name": "johnkrovitch/notification-bundle",
    "description": "Notification Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "johnkrovitch",
            "email": "arnaudfrezet@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "johnkrovitch/orm-pack": "^0.2",
        "symfony/property-access": "^3.0|^4.0",
        "symfony/yaml": "^3.0|^4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "phpunit/phpunit": "^8.4",
        "sensiolabs/security-checker": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "JK\\NotificationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JK\\NotificationBundle\\Tests\\": "tests/NotificationBundle/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "component-dir": "components",
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        }
    }
}
