{
    "name": "johnkrovitch/notification-bundle",
    "description": "Notification Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "johnkrovitch",
            "email": "arnaudfrezet@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/config": "^3.0 || ^4.0",
        "symfony/dependency-injection": "^3.0 || ^4.1.12"
    },
    "require-dev": {
        "symfony/flex": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "JK\\NotificationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JK\\NotificationBundle\\Tests\\": "tests/NotificationBundle/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "component-dir": "components",
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
